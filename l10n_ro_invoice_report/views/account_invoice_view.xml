<?xml version="1.0" ?>
<odoo>

        <record id="l10n_ro_invoice_form_view" model="ir.ui.view">
            <field name="name">l10n_ro.account.invoice.form</field>
            <field name="model">account.move</field>
            <field name="type">form</field>
            <field name="inherit_id" ref="account.view_move_form" />
            <field name="arch" type="xml">
                <xpath
                expr="//group[@id='header_left_group']"
                position="inside"
                invisible="move_type!='out_invoice'"
            >
                      <field name="delegate_id" readonly="state!='draft'" />
                      <field name="mean_transp" colspan="2" readonly="state!='draft'" />
                </xpath>
            </field>
        </record>
